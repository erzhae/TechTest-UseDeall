{
  "name": "auth",
  "version": "1.0.0",
  "license": "ISC",
  "main": "server.js",
  "scripts": {
    "code:format": "prettier --write ./src",
    "code:lint": "eslint ./src --ext .ts --fix",
    "clean": "rimraf dist/",
    "dev": "tsnd src/server.ts --respawn",
    "prebuild": "yarn clean",
    "build": "yarn prebuild && tsc -p . && yarn postbuild",
    "postbuild": "cp -r prisma/ dist/prisma/ && cp package.json dist/",
    "prestart": "prisma db push && prisma generate",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@prisma/client": "^4.1.0",
    "bcrypt": "^5.0.1",
    "bson": "^4.6.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.7.1",
    "prisma": "^4.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
